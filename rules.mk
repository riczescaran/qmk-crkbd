# Enable RGB underglow lighting
RGBLIGHT_ENABLE = yes

# Enable RGB Matrix (you don't want per-key RGB)
RGB_MATRIX_ENABLE = no

# Enable encoder support
ENCODER_MAP_ENABLE = yes

# Enable media/consumer keys
EXTRAKEY_ENABLE = yes

# Enable mouse keys
MOUSEKEY_ENABLE = yes

# Enable OLED
OLED_ENABLE = no

# Additional features
NKRO_ENABLE = yes
CONSOLE_ENABLE = no
COMMAND_ENABLE = no
